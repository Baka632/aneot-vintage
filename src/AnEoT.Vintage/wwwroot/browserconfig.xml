<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="images/tile/small-tile.png"/>
      <square150x150logo src="images/tile/medium-tile.png"/>
      <wide310x150logo src="images/tile/wide-tile.png"/>
      <square310x310logo src="images/tile/large-tile.png"/>
